class Solution(object):
    #solve the closing problem yet leaving the order 

    def ex(self,unit):
        test=True
        for x in range(len(unit)):
            if unit[x] =="(" :
                for i in range(x,len(unit)):
                    if unit[i] ==")":
                        break
                    if (i==(len(unit)-1)):
                        test=False
            elif unit[x] =="[" :
                    for i in range(x,len(unit)):
                        if unit[i] =="]":
                             break
                        if (i==(len(unit)-1)):
                            test=False
            elif unit[x] =="{" :
                    for i in range(x,len(unit)):
                        if unit[i] =="}":
                            break
                        if (i==(len(unit)-1)):
                            test=False
        return test
    def isValid(self, s):
        """
        :type s: str
        :rtype: bool
        """
        return self.ex(s)
        
